{
  "name": "wilchester-realty-group",
  "version": "1.0.0",
  "private": true,
  "description": "Web content for Wilchester Realty Group",
  "main": "index.js",
  "repository": "https://github.com/gwinchesters/wilchester-realty.git",
  "author": "George Smith",
  "license": "MIT",
  "scripts": {
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules styles/ -o styles/",
    "watch-css": "yarn build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "prerelease",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "prerelease",
    "bulma": "^0.7.1",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.11.1",
    "firebase": "^5.4.0",
    "immutable": "^3.8.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-google-maps": "^9.4.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "prettier": "^1.14.2"
  }
}
